{
  "framework": "mocha",
  "src_files": [
    "node_modules/chai/chai.js",
    "node_modules/sinon/pkg/sinon-1.7.3.js",
    "vendor/*",
    "lib/*",
    "test/setup.js",
    "test/*.test.js"
  ],
  "launchers": {
    "node": {
      "command": "npm run test:node",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "node"
  ]
}
